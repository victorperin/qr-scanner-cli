{
  "parser": "@typescript-eslint/parser",
  "env": {
    "node": true,
    "jest": true
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "prettier/prettier": ["error"],
    "no-console": "off",
    "comma-dangle": ["error", "always-multiline"],
    "import/prefer-default-export": "off",
    "import/no-unresolved": "off",
    "no-unused-vars": "off"
  }
}
